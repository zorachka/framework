{
    "name": "zorachka/framework",
    "description": "This is my package Application",
    "keywords": [
        "zorachka",
        "framework"
    ],
    "homepage": "https://github.com/zorachka/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "doctrine/migrations": "^3.1",
        "doctrine/sql-formatter": "^1.1",
        "finesse/swiftmailer-defaults-plugin": "^2.0",
        "laminas/laminas-diactoros": "^2.6",
        "laminas/laminas-httphandlerrunner": "^1.4",
        "league/route": "^5.0",
        "monolog/monolog": "^2.2",
        "ramsey/uuid": "^4.1",
        "sentry/sdk": "^3.1",
        "swiftmailer/swiftmailer": "^6.2",
        "twig/twig": "^3.3",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "files": [
            "src/Foundation/helpers.php"
        ],
        "psr-4": {
            "Zorachka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
