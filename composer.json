{
    "name": "zorachka/framework",
    "description": "Zorachka Framework",
    "keywords": [
        "zorachka",
        "framework"
    ],
    "homepage": "https://github.com/zorachka/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laminas/laminas-diactoros": "^2.8",
        "middlewares/fast-route": "^2.0",
        "middlewares/request-handler": "^2.0",
        "monolog/monolog": "^2.3",
        "narrowspark/http-emitter": "^2.0",
        "php-di/php-di": "^6.3",
        "relay/relay": "^2.1",
        "symfony/console": "^5.3",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Zorachka\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\Framework\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
